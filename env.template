# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Data Source APIs
HUNTER_API_KEY=your_hunter_api_key_here
SERPAPI_KEY=your_serpapi_key_here
GOOGLE_CSE_ID=your_google_cse_id_here
